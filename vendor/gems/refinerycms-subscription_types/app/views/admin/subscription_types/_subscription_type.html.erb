<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(subscription_type) -%>">
  <span class='title'>
    <%= subscription_type.title %>
    <span class="preview">&nbsp;</span>
  </span>
  <span class='actions'>
    <%= link_to refinery_icon_tag("application_go.png"), subscription_type_url(subscription_type),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    <%= link_to refinery_icon_tag("application_edit.png"), edit_admin_subscription_type_path(subscription_type),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), admin_subscription_type_path(subscription_type),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'shared.admin.delete', :title => subscription_type.title),
        :method => :delete %>
  </span>
</li>
