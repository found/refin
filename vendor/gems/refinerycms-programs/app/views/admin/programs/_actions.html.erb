<ul>
  <%# if Admin::ProgramsController.searchable? %>
    <!-- <li> -->
      <%#= render :partial => "/shared/admin/search",
                  :locals => {
                    :url => admin_programs_url
                  } %>
    <!-- </li> -->
  <%# end %>
  <li>
    <%= link_to t('.create_new'), new_admin_program_url,
                 :class => "add_icon" %>
  </li>
<% if !searching? and Admin::ProgramsController.sortable? and Program.count > 1 %>
  <li>
    <%= link_to t('.reorder', :what => "Programs"),
                 admin_programs_url,
                 :id => "reorder_action",
                 :class => "reorder_icon" %>

    <%= link_to t('.reorder_done', :what => "Programs"),
                 admin_programs_url,
                 :id => "reorder_action_done",
                 :style => "display: none;",
                 :class => "reorder_icon" %>
  </li>
<% end %>
</ul>