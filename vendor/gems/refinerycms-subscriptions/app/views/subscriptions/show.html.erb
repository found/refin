<% content_for :body_content_title do %>
  <%= @subscription.title %>
<% end %>

<% content_for :body_content_left do %>
  <section>
    <h1>Member</h1>
    <p>
      <%=raw @subscription.member_id %>
    </p>
  </section>
  <section>
    <h1>Subscription Type</h1>
    <p>
      <%=raw @subscription.subscription_type_id %>
    </p>
  </section>
<% end %>

<% content_for :body_content_right do %>
  <aside>
    <h2><%= t('.other') %></h2>
    <ul id="subscriptions">
      <% @subscriptions.each do |subscription| %>
        <li>
          <%= link_to subscription.title, subscription_url(subscription) %>
        </li>
      <% end %>
    </ul>
  </aside>
<% end %>

<%= render :partial => "/shared/content_page" %>
